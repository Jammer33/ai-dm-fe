{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./build",
      "moduleResolution": "node"
    },
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "node_modules",
      "build"
    ]
  }